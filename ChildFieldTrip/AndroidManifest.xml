<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.childfieldtrip"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.childfieldtrip.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.childfieldtrip.AnimalTitle"
            android:label="@string/title_activity_animal_title" >
        </activity>
        <activity
            android:name="com.example.childfieldtrip.Domestic"
            android:label="@string/title_activity_domestic" >
        </activity>
        <activity
            android:name="com.example.childfieldtrip.Dog"
            android:label="@string/title_activity_dog" >
        </activity>
        <activity
            android:name="com.example.childfieldtrip.CameraActivity"
            android:label="@string/title_activity_camera" >
        </activity>
        <activity
            android:name="com.example.childfieldtrip.Cat"
            android:label="@string/title_activity_cat" >
        </activity>
        <activity
            android:name="com.example.childfieldtrip.Reptiles"
            android:label="@string/title_activity_reptiles" >
        </activity>
        <activity
            android:name="com.example.childfieldtrip.Turtle"
            android:label="@string/title_activity_turtle" >
        </activity>
        <activity
            android:name="com.example.childfieldtrip.Rodents"
            android:label="@string/title_activity_rodents" >
        </activity>
        <activity
            android:name="com.example.childfieldtrip.Rabbit"
            android:label="@string/title_activity_rabbit" >
        </activity>
    </application>

</manifest>
